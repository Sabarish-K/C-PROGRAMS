/*Get a value N and accept N*N matrix and rotate in clockwise
eg Input:3
         1 2 3
         4 5 6
         7 8 9
         -----
 output: 7 4 1
         8 5 2
         9 6 3
*/            
#include <stdio.h>
#include <stdlib.h>
int main()
{
    int ;
    scanf("%d:,&n);
    int i,j,a[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d ",&a[i][j]);
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=n-1;j>=0;j--)
        {
            printf("%d ",a[j][i]);
        }
        printf("\n");
    }
    return 0;
}
